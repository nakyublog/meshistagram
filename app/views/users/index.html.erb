<% provide(:title, 'All users') %>
<h1>ユーザ一覧</h1>
<%= will_paginate %>


<h2>ユーザー検索</h2>
<%= form_tag(users_path, :method => 'get' ) do %>
  <%= text_field_tag :search, "", {class: "form-control"} %>
  <%= submit_tag '検索する', :name => nil, :class => "btn btn-success" %>
<% end %>

<ul class="users">
  <%= render @users %>
</ul>
<%= will_paginate %>